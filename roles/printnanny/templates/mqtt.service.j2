[Unit]
Description=Print Nanny MQTT Client
Requires=network.target
After=syslog.target network.target {{ updater_app_name }}-license.service
Wants={{ updater_app_name }}-license.service
[Service]
Type=simple
ExecStart=/usr/local/bin/printnanny mqtt
User={{ printnanny_user }}

[Install]
WantedBy=multi-user.target